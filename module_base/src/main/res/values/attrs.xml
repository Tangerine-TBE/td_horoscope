<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义加载框-->
    <declare-styleable name="LVCircularRing">
        <attr name="bgColor" format="color" />
        <attr name="barColor" format="color" />
    </declare-styleable>

    <!--自定义toolbar-->
    <declare-styleable name="MyToolbar">
        <attr name="title" format="string"/>
        <attr name="has_back" format="boolean"/>
        <attr name="has_add" format="boolean"/>
        <attr name="iconStyle" format="reference"/>
        <attr name="titleColor" format="color"/>
        <attr name="barBgColor" format="color"/>
        <attr name="rightTitle" format="string"/>
        <attr name="rightTitleColor" format="color"/>
        <attr name="hasRightTitle" format="boolean"/>

    </declare-styleable>

   <!--SmoothCheckBox-->
    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>

</resources>